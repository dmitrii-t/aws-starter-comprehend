{
  "name": "aws-starter-comprehend",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "aws-starter-comprehend": "./bin/CloudFormation.js"
  },
  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.ts",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack --config webpack.config.ts --watch --progress",
    "test": "jest",
    "cdk": "cdk",
    "submit": "node -e 'require(\"./bin/stream-connector/index.js\").main()'",
    "get": "node -e 'require(\"./bin/http-handler/index.js\").main()'"
  },
  "devDependencies": {
    "@aws-cdk/assets": "^0.27.0",
    "@aws-cdk/aws-apigateway": "^0.26.0",
    "@aws-cdk/aws-cloudwatch": "^0.26.0",
    "@aws-cdk/aws-ec2": "^0.26.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^0.26.0",
    "@aws-cdk/aws-elasticsearch": "^0.26.0",
    "@aws-cdk/aws-iam": "^0.26.0",
    "@aws-cdk/aws-kinesis": "^0.26.0",
    "@aws-cdk/aws-lambda": "^0.26.0",
    "@aws-cdk/aws-lambda-event-sources": "^0.26.0",
    "@aws-cdk/aws-s3": "^0.26.0",
    "@aws-cdk/cdk": "^0.26.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.11",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.0",
    "@types/webpack": "^4.4.26",
    "@types/webpack-node-externals": "^1.6.3",
    "@types/zip-webpack-plugin": "^3.0.0",
    "archiver": "^3.0.0",
    "aws-cdk": "^0.26.0",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "json-loader": "^0.5.7",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2",
    "zip-webpack-plugin": "^3.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.424.0",
    "axios": "^0.18.0"
  }
